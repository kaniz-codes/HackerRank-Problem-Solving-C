#include <stdio.h>

int main() 
{
    int a, b, sum = 0, min = 0;
    float x, y, sum1 = 0, min1 = 0;

    scanf("%d %d", &a, &b);
    scanf("%f %f", &x, &y);
    sum = a + b;
    min = a - b;
    printf("%d %d\n", sum, min);
    sum1 = x + y;
    min1 = x - y;
    printf("%.1f %.1f\n", sum1, min1);
    return 0;
}

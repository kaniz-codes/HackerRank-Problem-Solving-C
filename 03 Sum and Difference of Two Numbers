#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
	int a,b,sum,diff;
    float A,B,S,D;
    scanf("%d %d", &a, &b);
    scanf("%f %f", &A, &B);
    sum=a+b;
    diff=a-b;
    printf("%d %d\n",sum,diff);
    S=A+B;
    D=A-B;
    printf("%f %f\n",S,D);
    return 0;
}
